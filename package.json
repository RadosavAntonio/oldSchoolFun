{
  "name": "oldSchoolFun",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "clean": "yarn cache clean --all",
    "test": "jest",
    "tsc": "tsc --noEmit",
    "podUpdate": "cd ios && pod install --repo-update",
    "pod": "cd ios && pod install",
    "gradleClean": "cd android && ./gradlew clean",
    "fix-images": "npx react-native-fix-image",
    "i": "yarn && ( cd ios && bundle exec pod install --repo-update) && yarn fix-images",
    "cleani": "rm -rf node_modules && rm -rf ./ios/Pods && rm -rf ./ios/Podfile.lock && yarn cache clean && yarn && ( cd ios && bundle exec pod install ) && yarn fix-images",
    "kn": "killall node",
    "adb-r": "adb reverse tcp:8081 tcp:8081",
    "tron": "adb reverse tcp:9090 tcp:9090",
    "startReset": "yarn start -- --reset-cache",
    "node:clean": "rm -rf node_modules&& yarn cache clean && yarn install",
    "setDevelopment": "ENVFILE=.env.dev",
    "setProduction": "ENVFILE=.env.prod",
    "ios:prod": "yarn setProduction && npx react-native run-ios",
    "ios:dev": "yarn setDevelopment && npx react-native run-ios --scheme oldSchoolFunDev"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.0.0",
    "lodash": "^4.17.21",
    "react": "18.3.1",
    "react-native": "0.75.3",
    "react-native-config": "^1.5.3",
    "reactotron-react-native": "^5.1.8",
    "reactotron-redux": "^3.1.10"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.75.3",
    "@react-native/eslint-config": "0.75.3",
    "@react-native/metro-config": "0.75.3",
    "@react-native/typescript-config": "0.75.3",
    "@tsconfig/react-native": "^3.0.5",
    "@types/jest": "^29.5.13",
    "@types/lodash": "^4.17.9",
    "@types/react": "^18.3.9",
    "@types/react-test-renderer": "^18.3.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "18.3.1",
    "typescript": "^5.6.2"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@3.6.4"
}
